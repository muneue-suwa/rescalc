{
  "name": " rescalc",
  "description": "Google Chrome extension to calculate reservation start dates",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "calendar.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "storage"
  ]
}
